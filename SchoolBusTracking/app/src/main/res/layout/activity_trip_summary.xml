<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@android:color/white"
    tools:context="com.cgs.schoolbustracking.activities.TripSummary">
    <include
        android:id="@+id/toolbar"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        layout="@layout/toolbar_new">
    </include>
    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/toolbar"
        android:background="@android:color/white">
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:layout_margin="@dimen/tripsummary_table_margin"
        android:background="@android:color/black">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="@dimen/tripsummary_table_margin"
        android:gravity="center"
        android:orientation="vertical"
        android:background="@android:color/white">

   <LinearLayout
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:weightSum="2"
       android:orientation="horizontal">
       <TextView
           android:id="@+id/tripsummary_buslabel_txt"
           android:layout_width="0dp"
           android:layout_height="wrap_content"
           android:layout_weight="1"
           android:padding="@dimen/tripsummary_txtview_padding"
           style="@style/TextViewLargeStyle"
           android:gravity="center"
           android:text="Bus"/>

       <View
           android:layout_width="@dimen/tripsummary_view_width"
           android:layout_height="match_parent"
           android:background="@android:color/black"/>
       <TextView
           android:id="@+id/tripsummary_bus_txt"
           android:layout_width="0dp"
           android:layout_height="wrap_content"
           android:layout_weight="1"
           style="@style/TextViewLargeStyle"
           android:padding="@dimen/tripsummary_txtview_padding"
           android:gravity="center"
           android:text="test"/>

   </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/tripsummary_view_height"
            android:background="@android:color/black"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/tripsummary_timelabel_txt"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="@dimen/tripsummary_txtview_padding"
                style="@style/TextViewLargeStyle"
                android:gravity="center"
                android:text="Time"/>

            <View
                android:layout_width="@dimen/tripsummary_view_width"
                android:layout_height="match_parent"
                android:background="@android:color/black"/>
            <TextView
                android:id="@+id/tripsummary_time_txt"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                style="@style/TextViewLargeStyle"
                android:padding="@dimen/tripsummary_txtview_padding"
                android:gravity="center"
                android:text="test"/>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/tripsummary_view_height"
            android:background="@android:color/black"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/tripsummary_fromlabel_txt"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="@dimen/tripsummary_txtview_padding"
                style="@style/TextViewLargeStyle"
                android:gravity="center"
                android:text="From"/>

            <View
                android:layout_width="@dimen/tripsummary_view_width"
                android:layout_height="match_parent"
                android:background="@android:color/black"/>
            <TextView
                android:id="@+id/tripsummary_from_txt"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                style="@style/TextViewLargeStyle"
                android:padding="@dimen/tripsummary_txtview_padding"
                android:gravity="center"
                android:text="test"/>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/tripsummary_view_height"
            android:background="@android:color/black"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/tripsummary_tolabel_txt"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="@dimen/tripsummary_txtview_padding"
                style="@style/TextViewLargeStyle"
                android:gravity="center"
                android:text="To"/>

            <View
                android:layout_width="@dimen/tripsummary_view_width"
                android:layout_height="match_parent"
                android:background="@android:color/black"/>
            <TextView
                android:id="@+id/tripsummary_to_txt"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                style="@style/TextViewLargeStyle"
                android:padding="@dimen/tripsummary_txtview_padding"
                android:gravity="center"
                android:text="test"/>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/tripsummary_view_height"
            android:background="@android:color/black"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/tripsummary_totallabel_txt"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="@dimen/tripsummary_txtview_padding"
                style="@style/TextViewLargeStyle"
                android:gravity="center"
                android:text="Total"/>

            <View
                android:layout_width="@dimen/tripsummary_view_width"
                android:layout_height="match_parent"
                android:background="@android:color/black"/>
            <TextView
                android:id="@+id/tripsummary_total_txt"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                style="@style/TextViewLargeStyle"
                android:padding="@dimen/tripsummary_txtview_padding"
                android:gravity="center"
                android:text="test"/>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/tripsummary_view_height"
            android:background="@android:color/black"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/tripsummary_boardedlabel_txt"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="@dimen/tripsummary_txtview_padding"
                style="@style/TextViewLargeStyle"
                android:gravity="center"
                android:text="Boarded"/>

            <View
                android:layout_width="@dimen/tripsummary_view_width"
                android:layout_height="match_parent"
                android:background="@android:color/black"/>
            <TextView
                android:id="@+id/tripsummary_boarded_txt"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                style="@style/TextViewLargeStyle"
                android:padding="@dimen/tripsummary_txtview_padding"
                android:gravity="center"
                android:text="test"/>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/tripsummary_view_height"
            android:background="@android:color/black"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/tripsummary_missedlabel_txt"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="@dimen/tripsummary_txtview_padding"
                style="@style/TextViewLargeStyle"
                android:gravity="center"
                android:text="Missed"/>

            <View
                android:layout_width="@dimen/tripsummary_view_width"
                android:layout_height="match_parent"
                android:background="@android:color/black"/>
            <TextView
                android:id="@+id/tripsummary_missed_txt"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                style="@style/TextViewLargeStyle"
                android:padding="@dimen/tripsummary_txtview_padding"
                android:gravity="center"
                android:text="test"/>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/tripsummary_view_height"
            android:background="@android:color/black"/>

    </LinearLayout>
    </LinearLayout>
</ScrollView>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@color/ColorPrimary"
        android:text="Submit"/>

</RelativeLayout>
